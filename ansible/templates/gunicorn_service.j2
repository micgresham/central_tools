[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=central_tools
Group=www-data
WorkingDirectory=/{{vars.vars.directory}}/central_tools
ExecStart=/{{vars.vars.directory}}/central_tools/ct_venv/bin/gunicorn --access-logfile - --timeout 250  --workers 5 --bind unix:/tmp/core.sock core.wsgi:application

[Install]
WantedBy=multi-user.target
